<CustomLayer name="mhaOp" type="SimpleGPU" version="1">
    <Kernel entry="mha_opt">
        <Source filename="mha_op.cl"/>
    </Kernel>
    <Buffers><!-- use the same order as the kernel entry function, so in this case (input0,input1,output0,int,float,weights) -->
        <Tensor arg-index="0" type="input" port-index="0"/>
        <Tensor arg-index="1" type="input" port-index="1"/>
        <Tensor arg-index="2" type="input" port-index="2"/>
        <Tensor arg-index="3" type="output" port-index="0" format="BFYX"/>
    </Buffers>
    <CompilerOptions options="-cl-mad-enable"/>
    <WorkSizes global="F,144,X" local="1,1,X"/>
</CustomLayer>
